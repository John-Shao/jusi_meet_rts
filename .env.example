# Example environment variables for jusi_meet_rts
# Replace placeholders with real values in your local `.env` (DO NOT commit secrets)

# AK/SK configuration
ACCOUNT_ID=your_account_id_here
VOLC_AK=your_volc_access_key_here
VOLC_SK=your_volc_secret_key_here
VOLC_REGION=cn-beijing

# RTC configuration
RTC_APP_ID=your_rtc_app_id_here
RTC_APP_KEY=your_rtc_app_key_here

# Conversational AI app (optional)
VOLC_CAI_APP_ID=your_cai_app_id_here
VOLC_CAI_APP_KEY=your_cai_app_key_here

# Doubao S2S Realtime voice model (optional)
DOUBAO_S2S_APP_ID=your_doubao_app_id_here
DOUBAO_S2S_ACCESS_TOKEN=your_doubao_access_token_here

# RTMP/RTSP hosts and ports for forwarded streams
VIDEO_RTMP_HOST=service.jusiai.com
VIDEO_RTMP_PORT=1935
AUDIO_RTMP_HOST=service.jusiai.com
AUDIO_RTMP_PORT=2935
AUDIO_RTSP_PORT=8554

# Redis configuration
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=changeme

# MySQL configuration
MYSQL_HOST=127.0.0.1
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=changeme
MYSQL_DATABASE=jusi_db

# Others
DEBUG=False
